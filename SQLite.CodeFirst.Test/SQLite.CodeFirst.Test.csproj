    <PackageReference Include="System.Data.SQLite" Version="1.0.112" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.ComponentModel.DataAnnotations" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\SQLite.CodeFirst.Console\SQLite.CodeFirst.Console.csproj" />
